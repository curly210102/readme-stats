{
  "name": "readme-stats",
  "version": "0.1.0",
  "description": "Generate stats for readme",
  "main": "index.js",
  "scripts": {
    "create": "node scripts/create-new-card",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write \"./**/*.ts?(x)\"",
    "fix": "eslint --fix",
    "commit": "git-cz"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "prettier --write",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "author": "Curly Brackets",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/github-username-regex": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/node": "^1.12.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "word-wrap": "^1.2.3"
  }
}
